{
  "name": "limitless_mobile_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --experimental-debugger",
    "clean": "react-native clean",
    "format": "prettier --write \"src/**/*.ts\"",
    "gradlew": "cd android && ./gradlew clean && cd ..",
    "debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && yarn run post-debug",
    "post-debug": "cd android && ./gradlew clean && ./gradlew assembleDebug",
    "release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew clean && ./gradlew assembleRelease && cd .. && open android/app/build/outputs/apk/release",
    "bundle": "cd android && ./gradlew bundleRelease && open app/build/outputs/bundle/release",
    "dev": "ENV=dev && react-native run-android",
    "prod": "ENV=prod && react-native run-android",
    "dev-release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew clean  && ENV=dev && ./gradlew assembleRelease && cd .. && open android/app/build/outputs/apk/release",
    "prod-release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew clean  && ENV=prod && ./gradlew assembleRelease && cd .. && open android/app/build/outputs/apk/release"
  },
